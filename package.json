{
  "name": "pogo_network_server",
  "version": "0.1.0",
  "description": "poke go network back nodejs server",
  "main": "app/main.js",
  "engines": {
    "node": "5.8",
    "npm": ">=3.7"
  },
  "scripts": {
    "init_dev_project": "cd git-hooks-versionned/ && /bin/bash deployHooks.sh development &&  cd  ../ && npm i && psql -U postgres -h localhost -a -f db/initPokemonGoNetwork.database.sql",
    "export_dump_db":"pg_dump -U postgres -h localhost pokemonGoNetwork -f ./db/pokemonGoNetwork.bak.sql",
    "import_dump_db":"cat ./db/pokemonGoNetwork.bak.sql | psql -h localhost -U postgres pokemonGoNetwork",
    "dev": "ENV_DEFINITION=development nodemon --watch app/ app/main.js",
    "test": "echo \"Error: no test specified\" && exit 0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PogoNetwork/pogo_network_server.git"
  },
  "author": "bwinckell",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/PogoNetwork/pogo_network_server/issues"
  },
  "homepage": "https://github.com/PogoNetwork/pogo_network_server#readme",
  "dependencies": {
    "body-parser": "^1.15.2",
    "cookie-parser": "^1.4.3",
    "express": "^4.14.0",
    "express-session": "^1.14.0",
    "passport": "^0.3.2",
    "passport-google-oauth": "^1.0.0",
    "pg": "^6.0.3",
    "pg-pool": "^1.3.1"
  },
  "devDependencies": {
    "eslint": "^3.1.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-react": "^5.2.2",
    "jest": "^13.2.3",
    "nodemon": "^1.9.2"
  }
}
